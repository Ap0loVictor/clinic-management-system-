<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinicamed.controller.CadastrarConsultaController">
    <children>
        <Label layoutX="350.0" layoutY="14.0" text="Cadastrar Consulta">

            <font>
                <Font size="20.0" />
            </font>
        </Label>


        <Label layoutX="261.0" layoutY="70.0" text="Médico:" />
        <TextField fx:id="campoMedico" layoutX="320.0" layoutY="66.0" prefWidth="300.0" />

        <Label layoutX="257.0" layoutY="110.0" text="Paciente:" />
        <TextField fx:id="campoPaciente" editable="false" layoutX="320.0" layoutY="106.0" prefWidth="300.0" />

        <Label layoutX="257.0" layoutY="149.0" text="Data:" />
        <TextField fx:id="campoData" layoutX="320.0" layoutY="145.0" prefWidth="150.0" promptText="dd/MM/yyyy" />

        <Label layoutX="479.0" layoutY="149.0" text="Horário:" />
        <TextField fx:id="campoHorario" layoutX="530.0" layoutY="145.0" prefHeight="27.0" prefWidth="89.0" promptText="HH:mm" />

        <Label layoutX="253.0" layoutY="189.0" text="Descrição:" />
        <TextArea fx:id="campoDescricao" layoutX="320.0" layoutY="183.0" prefHeight="100.0" prefWidth="300.0" />

        <Button fx:id="salvarConsulta" layoutX="320.0" layoutY="306.0" onAction="#handleSalvarConsulta" prefHeight="29.0" prefWidth="300.0" text="Salvar Consulta" />
      <Button fx:id="voltar" layoutX="320.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleVoltar" prefHeight="29.0" prefWidth="300.0" text="Voltar" />

    </children>
</AnchorPane>
